query ArticleDetails($articleId: ID!, $hasArticleId: Boolean!, $blogsFirst: Int!) {
  onlineStore {
    availableTemplateOptions(resourceType: ARTICLE)
    article(id: $articleId) @include(if: $hasArticleId) {
      ...ArticleDetailsFragment
      blog {
        id
        title
        handle
        commentPolicy
        __typename
      }
      defaultCursor
      relativeStorefrontPath
      __typename
    }
    blogs(first: $blogsFirst) {
      nodes {
        value: id
        label: title
        handle
        __typename
      }
      __typename
    }
    currentTheme {
      id
      __typename
    }
    __typename
  }
  shop {
    id
    storefrontURL: url
    features {
      storefront
      __typename
    }
    resourceLimits {
      redirectLimitReached
      __typename
    }
    showAppLinks: appLinks(type: ARTICLES, location: SHOW, resourceId: $articleId) @include(if: $hasArticleId) {
      ...AppLinksFragment
      __typename
    }
    newAppLinks: appLinks(type: ARTICLES, location: NEW) @skip(if: $hasArticleId) {
      ...AppLinksFragment
      __typename
    }
    __typename
  }
  currentUser: staffMember {
    id
    name
    __typename
  }
}

fragment ArticleDetailsFragment on OnlineStoreArticle {
  id
  title
  body
  handle
  published
  publishDate
  summary
  seo {
    title
    description
    __typename
  }
  image {
    id
    altText
    src: url
    __typename
  }
  tags
  author {
    name
    __typename
  }
  templateSuffix
  __typename
}

fragment AppLinksFragment on AppLink {
  id
  text
  url
  icon {
    id
    originalSrc: url
    __typename
  }
  __typename
}
